{
  "tfstatePath": "~/repos/dev-ops/terraform/kna/terraform.tfstate",
  "serverTypes": {
    "k8s_master_kna": {
      "isMaster": true,
      "isBastionHost": true,
      "isMonitor": false,
      "user": "debian"
    },
    "k8s_node_kna": {
      "isMaster": false,
      "isBastionHost": false,
      "isMonitor": false,
      "user": "debian"
    },
    "k8s_monitoring_kna": {
      "isMaster": false,
      "isBastionHost": false,
      "isMonitor": true,
      "user": "debian"
    }
  },
  "ssh": {
    "configPath": "~/randomFiles/ssh_config",
    "user": "debian"
  },
  "ansible": {
    "hostsPath": "~/repos/acme-infrastructure/ansible/hosts"
  },
  "rke": {
    "configPath": "~/repos/acme-infrastructure/rke/cluster.yaml",
    "config": {
      "nodes": [],
      "services": {
        "etcd": {
          "snapshot": "true",
          "creation": "6h",
          "retention": "24h"
        },
        "kubelet": {
          "extra_args": {
            "cloud-provider": "external"
          }
        }
      },
      "network": {
        "plugin": "canal"
      },
      "authentication": {
        "strategy": "x509"
      },
      "authorization": {
        "mode": "rbac"
      },
      "ingress": {
        "provider": "none"
      }
    }
  }
}